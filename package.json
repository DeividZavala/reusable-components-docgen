{
  "name": "components-library-docgen",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "highlight.js": "^9.15.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "prestart": "npm run gen:docs",
    "start": "npm-run-all --parallel start:docs gen:docs-watch",
    "start:docs": "react-scripts start",
    "gen:docs": "node scripts/generateComponentData.js",
    "gen:docs-watch": "npm run gen:docs -- --watch",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "chokidar": "^2.1.5",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-docgen": "^4.1.0"
  }
}
